<% form_for @internal_rent, :url => {:action => "remove_equip"} do |f| %>
  <%= f.error_messages %>

  <b> Remove equipment: </b>
  <br />

  <%= hidden_field "agenda", "id", :id => @agenda.id %>

  <%= f.collection_select(:equip_id, @agenda.equips, :id, :classification, :class =>"Equip") %>
  <br /><br />

  <%= f.submit "Remove!" %>
<% end %>
